{

    "manifest_version": 2,
    "name": "CPC Crusher",
    "version": "1.0",
  
    "description": "Quickly shorten links throught cpc.cx",
  
    "permissions": [
      "tabs", "clipboardWrite", "notifications", "<all_urls>"
    ],

    "background": {
      "scripts": ["background/background.js"]
    },
  
    "browser_action": {
      "default_title": "CPC Crusher"
    }
  
}